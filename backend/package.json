{
  "name": "fleetflow-backend",
  "version": "1.0.0",
  "description": "FleetFlow Fleet & Logistics Management API",
  "main": "index.ts",
  "scripts": {
    "start": "npm run dev",
    "dev": "prisma generate && ts-node-dev --respawn --transpile-only index.ts --turbo",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:seed": "ts-node-dev prisma/seed.ts",
    "postinstall": "prisma generate",
    "queue:worker": "ts-node-dev src/queues/worker.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.30.0",
  "prisma": {
    "seed": "ts-node-dev prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.4.1",
    "@prisma/client": "^7.4.1",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.34.5",
    "cors": "^2.8.6",
    "express": "^5.2.1",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.18.0",
    "ts-node-dev": "^2.0.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/pg": "^8.16.0",
    "@types/serve-static": "^2.0.0",
    "dotenv": "^17.3.1",
    "jest": "^29.7.0",
    "prisma": "^7.4.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.2"
  }
}
